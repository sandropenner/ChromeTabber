{
    "manifest_version": 3,
    "name": "ChromeTabber",
    "version": "1.0",
    "description": "Cycles through tabs without reloading them.",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  